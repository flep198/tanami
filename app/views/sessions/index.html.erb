<p id="notice"><%= notice %></p>

<div class="w3-row-padding w3-padding-64 w3-container">
  <div class="w3-content">
  <h1>Sessions</h1>

  <table>
    <thead>
      <tr>
        <th>Data</th>
        <th>Comment</th>
        <th>Obs code</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @sessions.each do |session| %>
        <tr>
          <td><%= session.data %></td>
          <td><%= session.comment %></td>
          <td><%= session.obs_code %></td>
          <td><%= link_to 'Show', session %></td>
          <td><%= link_to 'Edit', edit_session_path(session) %></td>
          <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Session', new_session_path %>
</div>
</div>
