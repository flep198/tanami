<p id="notice"><%= notice %></p>
<div class="w3-row-padding w3-padding-64 w3-container">
  <div class="w3-content">
  <h1>Sources</h1>

  <table>
    <thead>
      <tr>
        <th>Be1950name</th>
        <th>Alt name</th>
        <th>Ra</th>
        <th>Decl</th>
        <th>Comment</th>
        <th>Atca link</th>
        <th>Source Type</th>
        <th>Redshift</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @sources.each do |source| %>
        <tr>
          <td><%= source.be1950name %></td>
          <td><%= source.alt_name %></td>
          <td><%= source.ra %></td>
          <td><%= source.decl %></td>
          <td><%= source.comment %></td>
          <td><%= source.atca_link %></td>
          <td><%= source.source_type %></td>
          <td><%= source.redshift %></td>
          <td><%= link_to 'Show', source %></td>
          <td><%= link_to 'Edit', edit_source_path(source) %></td>
          <td><%= link_to 'Destroy', source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Source', new_source_path %>
</div>
</div>